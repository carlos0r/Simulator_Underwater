<launch>
  <arg name="name" />
  <arg name="start_time" default="-1"/>
  <arg name="height" default="10"/>
  <arg name="width" default="20"/>
  <arg name="origin_x" default="0"/>
  <arg name="origin_y" default="0"/>
  <arg name="origin_z" default="-30"/>
  <arg name="n_points" default="20"/>
  <arg name="heading_offset" default="0"/>
  <arg name="max_forward_speed" default="0.3"/>

  <group ns="$(arg name)">
    <node pkg="control_utils"
      type="start_rectangle_trajectory.py"
      name="start_rectangle_trajectory"
      output="screen">
      <rosparam subst_value="true">
        start_time: $(arg start_time)
        height: $(arg height)
        width: $(arg width)
        origin:
          - $(arg origin_x)
          - $(arg origin_y)
          - $(arg origin_z)
        n_points: $(arg n_points)
        heading_offset: $(arg heading_offset)
        max_forward_speed: $(arg max_forward_speed)
      </rosparam>
    </node>
  </group>
</launch>
